<style>
  .as-seen-in {
    padding: 60px 0px;
  text-transform: unset !important;
}
  .as-seen-col {
    width: 50%;
    padding: 0px 0px;
}
  .as-seen-col-img img {
    max-width: 61%;
    height: 60px;
}
 
  @media screen and (max-width: 767px)
.as-seen-flex {
    flex-direction: inherit !important;
}
</style>
<section class="as-seen-in">
        <h4>{{section.settings.heading}}</h4>
        <div class="as-seen-flex">
            <div class="as-seen-col">
                <div class="as-seen-col-img">
                    <img src="{{section.settings.img | img_url:'master'}}" alt="the-quality-edit">
                </div>
            </div>
            <div class="as-seen-col">
                <div class="as-seen-col-img">
                    <img src="{{section.settings.img1 | img_url:'master'}}" alt="womens-republic">
                </div>
            </div> 
           <div class="as-seen-col">
                <div class="as-seen-col-img liast">
                    <img src="{{section.settings.img2 | img_url:'master'}}" alt="womens-republic">
                </div>
            </div> 
        </div>
    </section>

